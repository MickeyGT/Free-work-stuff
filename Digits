char l[3][256];
using namespace std;
void f(char l[3][255], int s)
{
	int i=2, j, k,nr=0,v[42],m[21][10],ok;
	memset(m, 0, sizeof(m));
	while ((l[1][i] != 'Ì')||(l[2][i] != 'Ì'))
	{
		nr++;
		k = 0;
		for (j = 1;j <= 2;j++)
		{
			if (l[j][i] != '|')
				ok = 0;
			else
				m[nr][k] = 1;
			k++;
		}
		for (j = 0;j <= 2;j++)
		{
			if (l[j][i-1] != '_' && l[j][i-1] != ' ')
				ok = 0;
			else
				if (l[j][i-1] == '_')
					m[nr][k] = 1;
			k++;
		}
		for (j = 1;j <= 2;j++)
		{
			if (l[j][i-2] != '|')
				ok = 0;
			else
				m[nr][k] = 1;
			k++;
		}
		i = i + 5;
	}
	///de aici transformare in numere si scazut din suma
	for (i = 0;i <= nr;i++)
	{
		if(m[i][0]&&m[i][1]&&m[i][3])

	}
	
}

int main(int argc, char **argv)
{
	ifstream in("cifre.in");

	char l[3][255];
	in.getline(l[0], 255);
	in.getline(l[1], 255);
	in.getline(l[2], 255);
	f(l, 43);
	system("pause");
	return 0;
}
