#include<fstream>
#include<string.h>
#include<ctype.h>
#include<iostream>
#include<algorithm>
#include<map>
#include<unordered_map>
#include<array>
#include<deque>
#include<queue>
#include<math.h>
#include<functional>
#include<unordered_set>
#include<set>
#include<iomanip>
#include<bitset>
using namespace std;
int n, t,k,i,m,x,y,ok,nr1,nr2;
string s;
int main()
{
    cin >> n;
    cin >> s;
    for (i = 0; i <= n-1; i++)
        if (s[i] == 'A')
            nr1++;
        else
            nr2++;
    if (nr1 == nr2)
        cout << "Friendship";
    else
        if (nr1 > nr2)
            cout << "Anton";
        else
            cout << "Danik";
    return 0;
}
