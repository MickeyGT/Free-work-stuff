#include <iostream>
#include <fstream>
#include <conio.h>
#include <stdlib.h>
#include <string.h>
#include <list>
#include <bitset>
#include <vector>

using namespace std;
int n;
unsigned short p[100][3];
void f(unsigned short n, unsigned short p[100][3])
{
	int a[10][10], i, j, l, c, t;
	for (i = 0;i <= 9;i++)
		for (j = 0;j <= 9;j++)
			a[i][j] = 0;
	i = 0;
	while (n > 0)
	{
		l = p[i][0];
		c = p[i][1];
		t = p[i][2];
		if ((l <= 9) && (l >= 0) && (c <= 9) && (c >= 0))
		{
			if ((t == 0) && (c > 1) && (c < 8) && (l < 6))
			{

				if (a[l][c] == 0 && a[l + 1][c] == 0 && a[l + 1][c - 1] == 0 && a[l + 1][c - 2] == 0 && a[l + 1][c + 1] == 0 && a[l + 1][c + 2] == 0 && a[l + 2][c] == 0 && a[l + 3][c] == 0 && a[l + 4][c] == 0 && a[l + 4][c + 1] == 0 && a[l + 4][c - 1] == 0)
				{
					a[l][c] = a[l + 1][c] = a[l + 1][c - 1] = a[l + 1][c - 2] = a[l + 1][c + 1] = a[l + 1][c + 2] = a[l + 2][c] = a[l + 3][c] = a[l + 4][c] = a[l + 4][c + 1] = a[l + 4][c - 1] = 1;
				}
			}
			if ((t == 1) && (c > 3) && (l < 8) && (l>1))
			{

				if (a[l][c] == 0 && a[l][c - 1] == 0 && a[l + 1][c - 1] == 0 && a[l + 2][c - 1] == 0 && a[l - 1][c - 1] == 0 && a[l - 2][c - 1] == 0 && a[l][c - 2] == 0 && a[l][c - 3] == 0 && a[l][c - 4] == 0 && a[l + 1][c - 4] == 0 && a[l - 1][c - 4] == 0)
				{
					a[l][c] = a[l][c - 1] = a[l + 1][c - 1] = a[l + 2][c - 1] = a[l - 1][c - 1] = a[l - 2][c - 1] = a[l][c - 2] = a[l][c - 3] = a[l][c - 4] = a[l + 1][c - 4] = a[l - 1][c - 4] = 1;
				}
			}
			if ((t == 2) && (c > 1) && (c < 8) && (l>3))
			{

				if (a[l][c] == 0 && a[l - 1][c] == 0 && a[l - 1][c + 1] == 0 && a[l - 1][c - 1] == 0 && a[l - 1][c + 2] == 0 && a[l - 1][c - 2] == 0 && a[l - 2][c] == 0 && a[l - 3][c] == 0 && a[l - 4][c] == 0 && a[l - 4][c - 1] == 0 && a[l - 4][c + 1] == 0)
				{
					a[l][c] = a[l - 1][c] = a[l - 1][c + 1] = a[l - 1][c - 1] = a[l - 1][c + 2] = a[l - 1][c - 2] = a[l - 2][c] = a[l - 3][c] = a[l - 4][c] = a[l - 4][c - 1] = a[l - 4][c + 1] = 1;
				}
			}
			if ((t == 3) && (l > 1) && (l < 8) && (c < 6))
			{

				if (a[l][c] == 0 && a[l][c + 1] == 0 && a[l + 1][c + 1] == 0 && a[l + 2][c + 1] == 0 && a[l - 1][c + 1] == 0 && a[l - 2][c + 1] == 0 && a[l][c + 2] == 0 && a[l][c + 3] == 0 && a[l][c + 4] == 0 && a[l + 1][c + 4] == 0 && a[l - 1][c + 4] == 0)
				{
					a[l][c] = a[l][c + 1] = a[l + 1][c + 1] = a[l + 2][c + 1] = a[l - 1][c + 1] = a[l - 2][c + 1] = a[l][c + 2] = a[l][c + 3] = a[l][c + 4] = a[l + 1][c + 4] = a[l - 1][c + 4] = 1;
				}
			}
		}
		n--;
		i++;
	}
	for (int i = 0;i <= 9;i++)
	{
		for (int j = 0;j <= 9;j++)
			if (a[i][j])
				cout << "[]";
			else
				cout << "..";
		cout << endl;
	}
}

int main(int argc, char **argv)
{
	ifstream in("av.in");
	in >> n;
	for (int i = 0; i < n; ++i)
		in >> p[i][0] >> p[i][1] >> p[i][2];
	f(n, p);
	cout << "penms0";
}
