#include<stdlib.h>
#include<stdio.h>
#include<fstream>
#include<string.h>
#include<vector>
#include<iostream>
#include<algorithm>
using namespace std;

char v[131080];
int n, k, l, j;
int i, ok, x;
int main()
{
	ifstream f("prefixe.in");
	ofstream g("prefixe.out");
	f >> n;
	for (k = 1; k <= n; k++)
	{
		f >> v;
		l = strlen(v);
		g << 0<<" ";
		for (j = 2; j <= l; j++)
		{
			for (i = j - 1; i >= 0; i--)
			{
				if (i == 0)
				{
					g << 0 << " ";
					break;
				}
				ok = 1;
				for (x = 0; x <= i - 1; x++)
					if (v[x] != v[j - i + x])
					{
						ok = 0;
						break;
					}
				if (ok)
				{
					g << i << " ";
					break;
				}
			}
		}
		g << '\n';
	}
	return 0;
}
