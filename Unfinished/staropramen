#include<fstream>
#include<ctype.h>
using namespace std;
int i, k, n, carry, t,prcarry;
char a, b;
struct str
{
	unsigned short int c1 : 5;
	unsigned short int c2 : 5;
	unsigned short int c3 : 5;
}s[167666];
//ifstream f("file.in");
//ofstream g("file.out");
ifstream f("staropramen.in");
ofstream g("staropramen.out");
int main()
{

	f >> t;

	for (k = 1; k <= t; k++)
	{
		f >> n;
		for (i = 0; i < 2*n; i += 2)
		{
			if (i % 6 == 0)
			{
				f >> a;
				f >> b;
				s[i / 6].c1 = a - '0' + b - '0';
			}
			else
				if (i % 6 == 4)
				{
					f >> a;
					f >> b;
					s[i / 6].c3 = a - '0' + b - '0';
				}
				else
					if (i % 6 == 2)
					{
						f >> a;
						f >> b;
						s[i / 6].c2 = a - '0' + b - '0';
					}
		}
		carry = 0;
		prcarry = 0;
		for (i = n-1; i >= 0; i--)
		{
			if (i % 3 == 0)
			{
				if (s[i / 3].c1 + prcarry >= 10)
					carry = 1;
				s[i / 3].c1 = (s[i / 3].c1 + prcarry) % 10;
				prcarry = carry;
			}
			else
				if (i % 3 == 2)
				{
					if (s[i / 3].c3 + prcarry >= 10)
						carry = 1;
					s[i / 3].c3 = (s[i / 3].c3 + prcarry) % 10;
					prcarry = carry;
				}
				else
					if (i % 3 == 1)
					{
						if (s[i / 3].c2 + prcarry >= 10)
							carry = 1;
						s[i / 3].c2 = (s[i / 3].c2 + prcarry) % 10;
						prcarry = carry;
					}
		}
		if (carry)
			g << 1;
		for(i=0;i<=n-1;i++)
		{
			if (i % 3 == 0)
			{
				g<<s[i / 3].c1;
			}
			else
				if (i % 3 == 2)
				{
					g<<s[i / 3].c3;
				}
				else
					if (i % 3 == 1)
					{
						g<<s[i / 3].c2;
					}
		}
		g << '\n';
	}
	return 0;
}
