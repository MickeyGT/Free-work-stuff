#include<fstream>
#include<cstring>
#include<queue>
#include<algorithm>
#include<string.h>
#include<bitset>
#include<ctime>
#include<set>
#include<cmath>
#include<unordered_map>
#include<iomanip>
#include<map>
#include<stack>
#include<vector>
#include<bitset>
#include<functional>
#include<iostream>
#include<deque>
#include<unordered_set>
#include<queue>
#include<list>
#include<array>
#include<ctype.h>
using namespace std;
int i, j, k, ok, nr, n, m, el, min1, max1, p, carry, t, sum;
char a,b;
struct str
{
	unsigned short int c1 : 5;
	unsigned short int c2 : 5;
	unsigned short int c3 : 5;
}s[170500];
ifstream f("file.in");
ofstream g("file.out");
//ifstream f("staropramen.in");
//ofstream g("staropramen.out");
int main()
{

	f >> t;

	for (k = 1; k <= t; k++)
	{
		f >> n;
		for (i = 0; i <= n; i+=3)
		{
			f >> a;
			f >> b;
			s[i/3].c1 = a-'0' + b-'0';
			if (i + 1 <= n)
			{
				f >> a;
				f >> b;
				s[i].c2 = a - '0' + b - '0';
			}
			if (i + 2 <= n)
			{
				f >> a;
				f >> b;
				s[i].c3 = a - '0' + b - '0';
			}
		}
		carry = 0;
		for (i = (2*n)/3; i >= 0; i --)
		{
			if (s[i].c3 + carry >= 10)
				carry = 1;
			g << (s[i].c3 + carry) % 10;
			if (s[i].c2 + carry >= 10)
				carry = 1;
			g << (s[i].c2 + carry) % 10;
			if (s[i].c1+carry >= 10)
				carry = 1;
			g << (s[i].c1 + carry) % 10;
		}
		if (carry)
			g << 1;
		g << '\n';
	}
	return 0;
}
