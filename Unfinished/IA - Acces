#include<fstream>
#include<string.h>
#include<ctype.h>
#include<iostream>
#include<algorithm>
#include<map>
#include<unordered_map>
#include<array>
#include<deque>
#include<unordered_set>
#include<set>
#include<math.h>
using namespace std;
int n, m, q, i, j, c, l, mat[1001][1001], beg, endc, k, nr,rez[1001][1001],number;
int main()
{
	ifstream f("file.in");
	ofstream g("file.out");
	f >> n >> m;
	for (i = 1; i <= n; i++)
		for (j = 1; j <= m; j++)
		{
			f >> mat[i][j];
			if (mat[i][j] == 0)
			{
				nr = 0;
				l = i;
				c = j;
				if (i == 1)
				{
					while (c >= 1 && mat[l][c--] != 1)
						nr++;
					rez[i][j] = nr;
				}
				else
				{
					while (c >= 1 && mat[l][c--] != 1)
						nr++;
					c = j;
					number = nr;
					while (c >= 1 && mat[l - 1][c] == 1)
					{
						c--;
						number--;
					}
					for (k = c - 1; k >= c - number + 1; k--)
						if (mat[l - 1][k] == 1 && mat[l - 1][k - 1] == 0 && k- 1 >= 1)
						{
							beg = l - 1;
							while (mat[beg][k - 1] == 0 && mat[beg][k] == 1)
							{
								nr++;
								beg--;
							}
						}
					nr += rez[l - 1][c];
					rez[i][j] = nr;
				}
			}
		}
	f >> q;
	for (k = 1; k <= q; k++)
	{
		f >> l >> c;
		g << rez[l][c]<<"\n";
	}
	return 0;
}
