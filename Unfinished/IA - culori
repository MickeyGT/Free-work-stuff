#include<fstream>
#include<string.h>
#include<ctype.h>
#include<iostream>
#include<algorithm>
#include<map>
#include<unordered_map>
#include<array>
#include<deque>
#include<unordered_set>
#include<set>
#include<math.h>
using namespace std;
void adunare(int x[7000],int y[7000],int z[7000])
{
	int lenx = x[0];
	int leny = y[0];
	int cifre = max(leny, lenx);
	int carry = 0;
	for (i = 1; i <=cifre; i++)
	{
		z[i] = (x[i] + y[i]+carry) % 10;
		if (x[i] + y[i] >= 10)
			carry = 1;
		else
			carry = 0;
	}
	if (x[cifre] + y[cifre] >= 10)
	{
		z[0] = cifre + 1;
		z[cifre + 1] = 1;
	}
	else
	{
		z[0] = cifre;
	}
}
int n, i, total[7000],prv[6][7000],curr[6][7000],j,nr;
int main()
{
	ifstream f("culori3.in");
    ofstream g("culori3.out");
	f >> n;
	prv[1][0] = 1;
	prv[1][1] = 5;
	prv[2][0] = 1;
	prv[2][1] = 6;
	adunare(prv[1], prv[2], prv[3]);
	g << total;
	return 0;
}
