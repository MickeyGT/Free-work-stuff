#include<fstream>
#include<string.h>
#include<ctype.h>
#include<iostream>
#include<algorithm>
#include<map>
#include<unordered_map>
#include<array>
#include<deque>
#include<unordered_set>
using namespace std;
vector<pair<int, int>>v[50010];
deque<int>que;
int n, m, i,nstart,nend,cost,cmin[50010];
bool incoada[50010];
int main()
{
	ifstream f("file.in");
	ofstream g("file.out");
	f >> n >> m;
	for (i = 1; i <= m; i++)
	{
		f >> nstart >> nend >> cost;
		v[nstart].push_back(make_pair(nend,cost));
	}
	cmin[1] = 0;
	que.push_back(1);
	return 0;
}
