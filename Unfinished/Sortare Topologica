using namespace std;
struct nod
{
	int currnod;
	vector<nod> vecini;
}v[50500];
int i, n, m, j, k, nr,x,y,start;
deque<int>que;
int main()
{
	//ifstream f("file.in");
	//ofstream g("file.out");
	ifstream f("noroc.in");
	ofstream g("noroc.out");
	f >> n >> m;
	f >> x >> y;
	start = x;
	v[x].vecini.push_back(v[y]);
	for (i = 2; i <= m; i++)
	{
		f >> x >> y;
		v[x].vecini.push_back(v[y]);
	}
	que.push_back(start);
	while()
	return 0;
}
