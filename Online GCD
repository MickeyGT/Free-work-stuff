#include <cstdio>
#include <iostream>
#include<math.h>
int n, m, i, nr, marime, marimein, j, gcduri,ok,ok1;
long v[100011], value, a[100010], gcd;
using namespace std;
int findnearest2power(int el)
{
	for (int i = 1;i <= 42424242;i++)
		if (pow(2, i) > el)
			return pow(2, i - 1);
}
long gcd1(long el1, long el2)
{
	while (el1 != el2)
		if (el1>el2)
			el1 = el1 - el2;
		else
			el2 = el2 - el1;
	return el1;
}
int main()
{
	cin >> n >> m;
	for (i = 1;i <= n;i++)
		cin >> v[i];

	for (i = 1;i <= m;i++)
	{
		ok = 0;
		cin >> nr >> value;
		v[nr] = v[nr] / value;
		marime = n;
		ok1 = 1;
		for (j = 1;j <= n;j++)
			a[j] = v[j];
		while (1) {
			if (ok)
				break;
		
			gcduri = 1;
	
	
			for (j = 1;j <=marime-1;j = j++)
			{
				a[gcduri] = gcd1(a[j], a[j + 1]);
				if (a[gcduri] == 1)
				{
					cout << "1\n";
					break;
					ok = 1;
				}
				gcduri++;
			}
			
			gcduri--;
			marime = gcduri;
			
			if ((gcduri == 2)&&(ok==0))
			{
				cout << gcd1(a[1], a[2]) << "\n";
				break;
				ok = 1;
			}
			
		}
	}
	system("pause");
}
