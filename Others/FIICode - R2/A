using namespace std;
long long i, j, k, ok, nr, m, el, min1, max1, x, y, a, b;
string n;
int main()
{
	//ifstream f("file.in");
	//ofstream g("file.out");
	cin >> n;
	for(i=0;i<=n.size()-1;i++)
	{
		el = n[i] - '0';
		if (el == 0 || el == 9 || el == 6)
			nr++;
		else
			if (el == 8)
				nr += 2;
	}
	cout << nr;
	return 0;
}
