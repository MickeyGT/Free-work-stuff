using namespace std;
int i, j, k, n, nr, m,v[10100];


int main()
{
	//ios_base::sync_with_stdio(false);
	cin >> n;
	for (i = 1; i <= n; i++)
		nr += i;
	if (nr % 2 == 1)
		cout << "grimy";
	else
		cout << "black";
	return 0;
}
