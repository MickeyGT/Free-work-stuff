#include<stdlib.h>
#include<stdio.h>
#include<iostream>
#include<string.h>
#include<vector>
#include<algorithm>
using namespace std;
vector <int> v;
vector <int>::iterator it,it2;
int n, i,el,pos,nr;
int binarysearch(vector <int> v, int in, int sf, int m)
{
	int max = 1 << 31;
	while (v.begin()+in-1 <= v.begin()+sf-1)
	{
		int mid = (in + sf) / 2;
		it = v.begin() + mid;
		if(*it >= m)
		{
			max = mid;
			sf = mid - 1;
		}
		else if (*it < m)
			in = mid + 1;
	}
	return max;
}
int main()
{
	cin >> n;
	for (i = 1;i <= n;i++)
	{
		cin >> el;
		v.push_back(el);
	}
	cin >> nr;
	for (i = 1;i <= nr;i++)
	{
		cin >> el;
		pos = binarysearch(v, 0, n, el);
		it2 = v.begin() + pos;
		if (*it2 == el)
		{
			cout << "Yes " << pos+1 << endl;
		}
		else
		{
			cout << "No " << pos+1 << endl;
		}
	}
	
	return 0;
}
