#include<array>
#include<ctype.h>
using namespace std;
int i, j, k, ok, nr, n, m, el, min1, max1, x, y, a, b,v[100100];
int main()
{
	cin >> a >> b;
	for (i = a; i <= b; i++)
		if (i % 2 == 1)
		{
			v[i] = 1;
		}
		else
			if ((i - 1) % 3 == 0)
				v[i] = 1;
	for (i = a; i <= b; i++)
		if (v[i] == 0)
			nr++;
	cout << nr;
	return 0;
}
