using namespace std;
long long i, j, k, x, y, z, ok, nr, n, m, el, min1, max1, cost, a, b;
int main()
{
	cin >> k;
	while (k >= 50)
	{
		nr++;
		k -= 50;
	}
	while (k >= 5)
	{
		nr++;
		k -= 5;
	}
	nr += k;
	cout << nr;
	return 0;
}
