#include <iostream>
#include <fstream>
#include <conio.h>
#include <stdlib.h>
#include <string.h>
#include <list>
#include <bitset>
#include <vector>
using namespace std;
unsigned int g1, g2, g3;
void f(unsigned char jan, unsigned char jac, unsigned char jbn,	unsigned char jbc, bool r)
{
	static int v[99], juccur, prevcard, ok = 1, pointsa, pointsb,rounds,roundpoints;
	if (g1 == 0)
	{
		g1 = 1;
		juccur = 1;
		pointsa = 0;
		pointsb = 0;
		rounds = 0;
		roundpoints = 0;
		prevcard = 0;
	}
	if ((v[8 * int(jac) + int(jan)] == 1) || (v[8 * int(jbc) + int(jbn)] == 1))
		cout << "Invalid\n";
	else
	{
		rounds++;
		v[14* int(jac) + int(jan)] = 1;
		v[14 * int(jbc) + int(jbn)] = 1;
		if ((v[14 * int(jac) + int(jan)] == 10) || (v[14 * int(jac) + int(jan)] == 11))
			roundpoints++;
		if ((v[14 * int(jbc) + int(jbn)] == 10) || (v[14 * int(jbc) + int(jbn)] == 11))
			roundpoints++;
		if (juccur == 2)
		{
			swap(jac, jbc);
			swap(jan, jbn);
		}
		if (rounds != 16)
				if (r == 1)
				{
					if ((prevcard == jbn) || (jbn == 7))
						if (juccur == 1)
							cout << "Jucator curent:a\n";
						else
							cout << "Jucator curent:b\n";
					else
					{
						if (juccur == 1)
						{
							cout << "Jucator curent:a\n";
							juccur = 2;
							pointsb += roundpoints;
							roundpoints = 0;
						}
						else
						{
							cout << "Jucator curent:b\n";
							juccur = 1;
							pointsa += roundpoints;
							roundpoints = 0;
						}
					}
				}
				else
				{
					if ((jan == jbn) || (jbn == 7))
						if (juccur == 1)
							cout << "Jucator curent:a\n";
						else
							cout << "Jucator curent:b\n";
					else
					{
						if (juccur == 1)
						{
							cout << "Jucator curent:a\n";
							juccur = 2;
							pointsb += roundpoints;
							roundpoints = 0;
						}
						else
						{
							cout << "Jucator curent:b\n";
							juccur = 1;
							pointsa += roundpoints;
							roundpoints = 0;
						}
					}
				}
		else
			if (r == 1)
			{
				if ((prevcard == jbn) || (jbn == 7))
					if (juccur == 1)
					{
						g1 = 0;
						pointsb += roundpoints;
						cout << "Final joc:" << pointsa << "-" << pointsb << endl;
					}
				else
				{
					if (juccur == 1)
						pointsb += roundpoints;
					else
						pointsa += roundpoints;
					cout << "Final joc:" << pointsa << "-" << pointsb << endl;
				}
			}
	}

}
int main(int argc, char **argv)
{
	ifstream g("a.in");
	g1 = 0, g2 = 0, g3 = 0;
	unsigned int e1, e2, e3, e4, e5;
	while (g >> e1 >> e2 >> e3 >> e4 >> e5)
		f(e1, e2, e3, e4, e5 );
		return 0;
}


