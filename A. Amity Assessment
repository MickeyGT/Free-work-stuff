#include <stdlib.h>
#include<stdio.h>
#include<iostream>
#include<string.h>
#include<ctype.h>
using namespace std;
char m1[3][3], m2[3][3],string1[4],string2[4],string3[4],string4[4];
int i, j,nr;
int main()
{
	for (i = 1;i <= 2;i++)
		for (j = 1;j <= 2;j++)
			cin >> m1[i][j];
	for (i = 1;i <= 2;i++)
		for (j = 1;j <= 2;j++)
			cin >> m2[i][j];
	for (i = 1;i <= 2;i++)
		for (j = 1;j <= 2;j++)
			if (m1[i][j] == 'X')
			{
				if (j == 1)
				{
					if (i == 1)
					{
						string1[0] = m1[2][1];
						string1[1] = m1[2][2];
						string1[2] = m1[1][2];

						string2[0] = m1[1][1];
						string2[1] = m1[1][2];
						string2[2] = m1[2][2];
					}
					else
						if (i == 2)
						{
							string1[0] = m1[1][1];
							string1[1] = m1[1][2];
							string1[2] = m1[2][2];

							string2[0] = m1[2][1];
							string2[1] = m1[2][2];
							string2[2] = m1[1][2];

						}
				}
				else
					if(j==2)
					{
						if (i == 1)
						{
							string1[0] = m1[1][1];
							string1[1] = m1[2][1];
							string1[2] = m1[2][2];

							string2[0] = m1[2][2];
							string2[1] = m1[2][1];
							string2[2] = m1[1][1];
						}
						else
							if (i == 2)
							{
								string1[0] = m1[2][1];
								string1[1] = m1[1][1];
								string1[2] = m1[1][2];

								string2[0] = m1[1][2];
								string2[1] = m1[1][1];
								string2[2] = m1[2][1];

							}
					}
	

				

			}
	for (i = 1;i <= 2;i++)
		for (j = 1;j <= 2;j++)
			if (m2[i][j] == 'X')
			{
				if (j == 1)
				{
					if (i == 1)
					{
						string3[0] = m2[2][1];
						string3[1] = m2[2][2];
						string3[2] = m2[1][2];

						string4[0] = m2[1][2];
						string4[1] = m2[2][2];
						string4[2] = m2[2][1];
					}
					else
						if (i == 2)
						{
							string3[0] = m2[1][1];
							string3[1] = m2[1][2];
							string3[2] = m2[2][2];

							string4[0] = m2[2][2];
							string4[1] = m2[1][2];
							string4[2] = m2[1][1];

						}
				}
				else
					if (j == 2)
					{
						if (i == 1)
						{
							string3[0] = m2[1][1];
							string3[1] = m2[2][1];
							string3[2] = m2[2][2];

							string4[0] = m2[2][2];
							string4[1] = m2[2][1];
							string4[2] = m2[1][1];
						}
						else
							if (i == 2)
							{
								string3[0] = m2[2][1];
								string3[1] = m2[1][1];
								string3[2] = m2[1][2];

								string4[0] = m2[1][2];
								string4[1] = m2[1][1];
								string4[2] = m2[2][1];

							}
					}




			}
	_strrev(string3);
	_strrev(string4);
	nr = 0;
	if (!(strcmp(string1, string3) == 0))
		nr++;
    if (!(strcmp(string2, string4) == 0))
			nr++;
	if (strcmp(string2, string3) != 0)
				nr++;
	if (strcmp(string1, string4) != 0)
					nr++;
	
	if (nr == 4)
		cout << "YES";
	else
		cout << "NO";

	return 0;
}
