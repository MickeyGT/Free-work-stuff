#include <fstream>
#include <iostream>
#include <iomanip>
#include <unordered_set>

long long el,V,Q,nr, i, n, m, A, B, C, D, E;
using namespace std;

int main()
{
	ifstream f("muzica.in");
	ofstream g("muzica.out");
	unordered_set< long, hash < long > > M;
	unordered_set<long>::iterator it;
	f >> n;
	f >> m;
	f >> A >> B >> C >> D >> E;
	for (i = 1;i <= n;i++)
	{
		f >> el;
		M.insert(el);
	}
	nr = 0;
	el = 0;
	it = M.find(A);
	if (it != M.end())
	{
		nr++;
		M.erase(it);
	}
	it = M.find(B);
	if (it != M.end())
	{
		nr++;
		M.erase(it);
	}
	V = B;
	Q = A;
	for (i = 3;i <= m-1;i++)
	{
		el = (C*V + D*Q) % E;
		it = M.find(el);
		if (it != M.end())
		{
			nr++;
			M.erase(it);
		}
		Q = V;
		V = el;

	}

	g << nr;

	


	return 0;
}
